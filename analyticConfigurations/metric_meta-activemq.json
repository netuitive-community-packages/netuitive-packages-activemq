{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "activemq\\.org\\.apache\\.activemq\\.Broker\\.brokerName\\..*\\.QueueEnqueueCount",
        "properties": {
          "STATISTIC": "sum",
          "tags": {
            "unit": "count"
          },
          "type": "COUNTER"
        }
      }
    ],
    "name": "ActiveMQ",
    "scope": {
      "elementType": "SERVER",
      "metricMatches": "^activemq\\..*"
    },
    "type": "METRIC_META"
  }
}